{
    "name": "kintone",
    "apiVersion": "1.0",
    "description": "kintone REST API client",
    "operations": {
        "GetFormFields": {
            "httpMethod": "GET",
            "uri": "form.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "query"
                }
            }
        },
        "GetRecord": {
            "httpMethod": "GET",
            "uri": "record.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "query"
                },
                "id": {
                    "description": "record id",
                    "location": "query"
                }
            }
        },
        "PostRecord": {
            "httpMethod": "POST",
            "uri": "record.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "json"
                },
                "record": {
                    "description": "record data",
                    "location": "json"
                }
            }
        },
        "PostRecords": {
            "httpMethod": "POST",
            "uri": "records.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "json"
                },
                "records": {
                    "description": "record data",
                    "location": "json"
                }
            }
        },
        "PutRecord": {
            "httpMethod": "PUT",
            "uri": "record.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "json"
                },
                "id": {
                    "description": "record id",
                    "location": "json"
                },
                "revision": {
                    "description": "revision number",
                    "location": "json"
                },
                "record": {
                    "description": "record data",
                    "location": "json"
                }
            }
        },
        "DeleteRecord": {
            "httpMethod": "DELETE",
            "uri": "record.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "json"
                },
                "ids": {
                    "description": "record ids",
                    "location": "json"
                },
                "revisions": {
                    "description": "revision numbers",
                    "location": "json"
                }
            }
        },
        "GetRecords": {
            "httpMethod": "GET",
            "uri": "records.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "query"
                },
                "query": {
                    "description": "kintone find record query",
                    "location": "query"
                },
                "fields": {
                    "description": "kintone app fields",
                    "location": "query"
                }
            }
        },
        "GetFile": {
            "httpMethod": "GET",
            "uri": "file.json",
            "responseClass": "Cstap\\Kintone\\Response\\RawResponse",
            "parameters": {
                "fileKey": {
                    "description": "file key",
                    "location": "query"
                }
            }
        },
        "GetApp": {
            "httpMethod": "GET",
            "uri": "app.json",
            "parameters": {
                "id": {
                    "description": "kintone app id",
                    "location": "query"
                }
            }
        },
        "GetApps": {
            "httpMethod": "GET",
            "uri": "apps.json",
            "parameters": {
                "ids": {
                    "description": "list of kintone app id",
                    "location": "query"
                },
                "codes": {
                    "description": "list of kintone app code",
                    "location": "query"
                },
                "name": {
                    "description": "(part of) kintone app name",
                    "location": "query"
                },
                "spaceIds": {
                    "description": "list of space id",
                    "location": "query"
                },
                "limit": {
                    "description": "limit",
                    "location": "query"
                },
                "offset": {
                    "description": "offset",
                    "location": "query"
                }
            }
        },
        "GetSpace": {
            "httpMethod": "GET",
            "uri": "space.json",
            "parameters": {
                "id": {
                    "description": "kintone space id",
                    "location": "query"
                }
            }
        }
    }
}
