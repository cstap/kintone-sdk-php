{
    "name": "kintone",
    "apiVersion": "1.0",
    "description": "kintone REST API client",
    "operations": {
        "GetFormFields": {
            "httpMethod": "GET",
            "uri": "form.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "query"
                }
            }
        },
        "GetRecord": {
            "httpMethod": "GET",
            "uri": "record.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "query"
                },
                "id": {
                    "description": "record id",
                    "location": "query"
                }
            }
        },
        "GetRecords": {
            "httpMethod": "GET",
            "uri": "records.json",
            "parameters": {
                "app": {
                    "description": "kintone app id",
                    "location": "query"
                },
                "query": {
                    "description": "kintone find record query",
                    "location": "query"
                },
                "fields": {
                    "description": "kintone app fields",
                    "location": "query"
                }
            }
        }
    }
}
